{
  "name": "ddm-selecty",
  "version": "1.0.1",
  "description": "A select component patterned after selectize",
  "main": "./dist/ddm.selecty.js",
  "scripts": {
    "build:examples": "babel-node --presets es2015 examples/build.js",
    "build:umd": "webpack src/index.js dist/ddm.selecty.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/ddm.selecty.min.js --config webpack.config.production.js",
    "watch": "npm run build:umd -- --watch",
    "build": "npm run build:umd && npm run build:umd:min",
    "check": "npm run lint && npm run test",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test examples",
    "lint:fix": "eslint ./src/ --ext .js --ext .jsx --fix --cache",
    "preversion": "npm run clean && npm run check",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "test": "cross-env NODE_ENV=test karma start testing/karma.conf.js --single-run",
    "test:examples": "babel-node examples/tests.js",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "version": "npm run build"
  },
  "pre-commit": {
    "run": [
      "lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deseretdigital-ui/ddm-selecty.git"
  },
  "keywords": [
    "ddm",
    "react",
    "rating",
    "star",
    "ddm-selecty"
  ],
  "author": "Bryce Hayden (http://github.com/QuietOment)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deseretdigital-ui/ddm-selecty/issues"
  },
  "homepage": "https://github.com/deseretdigital-ui/ddm-selecty#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-css-modules": "^3.7.9",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "0.5.0",
    "cq-prolyfill": "^0.3.2",
    "cross-env": "2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.1.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isparta": "^4.0.0",
    "isparta-loader": "2.0.0",
    "json-loader": "^0.5.4",
    "karma": "1.2.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "node-libs-browser": "^1.0.0",
    "null-loader": "^0.1.1",
    "postcss-autoreset": "^1.1.5",
    "postcss-initial": "^1.5.2",
    "postcss-loader": "^0.9.1",
    "pre-commit": "^1.1.3",
    "precss": "^1.4.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "rimraf": "^2.5.0",
    "sinon": "2.0.0-pre.2",
    "skin-deep": "^0.16.0",
    "style-loader": "^0.13.0",
    "svg-injector": "^1.1.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11"
  },
  "npmName": "ui-rating",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
